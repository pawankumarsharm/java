		Scanner sc=new Scanner(System.in);
		System.out.println("enter the number");
		int n=sc.nextInt();
		int i=0;
		while(i<n/2)
		{
			int r=n%10;
			pro=pro+r*r;
			n=n/10;
			
		}
		System.out.println("products of digits:"+pro);
