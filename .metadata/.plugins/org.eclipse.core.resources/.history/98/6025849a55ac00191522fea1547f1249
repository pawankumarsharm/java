package Array;

import java.util.Scanner;

public class countdigitinnnumber {

	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		System.out.println("enter the number");
		int n=sc.nextInt();
		int count=1;
		do
		{
			count++;
			n=n/10;
		}
		while(n!=0);
		System.out.println("number of digits:"+count);
	}

}
