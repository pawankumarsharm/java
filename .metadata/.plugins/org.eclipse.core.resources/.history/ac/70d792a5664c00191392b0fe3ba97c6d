package jspider;

import java.util.Scanner;

public class p22 {

	public static void main(String[] args) {
		//Scanner sc=new Scanner(System.in);
		//int n=sc.nextInt();
		int n=5;
		for(int i=1;i<=n;i++)
		{
	    for(int j=1;j<=n-i;j++)
		{
	    	System.out.println(" ");
		}
	    for(int k=n-i;k<n;k++) {
	    	
			System.out.print((char)(97+k)+" ");

	}
	    System.out.println();	

}
		}
	}